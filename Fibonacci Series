def fib(n):
    if n==0:
        print("0")
    elif n==1:
        print("0")
    elif n==2:
        print("0",",","1")
        
    else:
        i=1
        print("0",",","1")
        while i<=n:
            print(fib(n-2)+fib(n-1),",")

num=int(input("Enter number of fibonacci series:"))
print(fib(num))  
